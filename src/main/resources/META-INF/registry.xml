<idea-plugin>
	<extensions defaultExtensionNs="com.intellij">
		<registryKey
			id="insyncwithfoo.ryecharm.logging.commands"
			key="insyncwithfoo.ryecharm.logging.commands"
			defaultValue="false"
			description="RyeCharm: Log all commands and their outputs"
		/>
		
		<!--suppress PluginXmlCapitalization -->
		<registryKey
			id="insyncwithfoo.ryecharm.redknot.panels"
			key="insyncwithfoo.ryecharm.redknot.panels"
			defaultValue="false"
			description="RyeCharm: Show Red Knot panels in settings"
		/>
	</extensions>
</idea-plugin>
